{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "no-console": false,
        "member-access": false,
        "object-curly-spacing": true,
        "semicolon": true,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}